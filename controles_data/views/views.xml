<odoo>
    <data>
        <record id="control_calidad_tree_view" model="ir.ui.view">
            <field name="name">control.calidad.tree.view</field>
            <field name="model">quality.check</field>
            <field name="inherit_id" ref="quality_control.quality_check_view_tree"></field>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='quality_state']" position="after">
                    <field name="measure"/>
                    <field name="norma"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>